class Main {
  	public static void main(String[] args) {
  		String s = "hello";
    	System.out.println(s);
  }
}

//

import java.lang.*;


class Main {
  	public static void main(String[] args) {
  		String s = "hello";
  			String s2 = s.toUpperCase();
  			String s3 = s.toLowerCase();

    	System.out.println(s);
    		System.out.println(s2);
    		System.out.println(s3);
  }
}


//

import java.lang.*;


class Main {
  	public static void main(String[] args) {
  		String s = "hello";
  			String s2 = s.toUpperCase();
  			String s3 = s.toLowerCase();
  				int j = s.length();
  					String i = s.substring(0,3);

    	System.out.println(s);
    		System.out.println(s2);
    		System.out.println(s3);
    			System.out.print(j);
    				System.out.print(i);
  }
}


