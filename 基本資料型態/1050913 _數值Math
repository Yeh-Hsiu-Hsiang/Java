class Main {
  public static void main(String[] args) {

	double i = Math.sqrt(15); //開根號
		System.out.println(i);

  }
}


//


class Main {
  public static void main(String[] args) {

	int i =Math.abs(15);  //絕對值
	int j =Math.abs(-15);

		System.out.println(i);
		System.out.println(j);

  }
}

      //	int i , j;
			i = Math.abs(15);
			j = Math.abs(-15);

				System.out.println(i);
				System.out.println(j);


