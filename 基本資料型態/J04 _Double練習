import java.util.*;

class Main {
    public static void main(String[] args) {

   		int price=200;
   		int age = 18;
   		String status="學生";
   			if(age<20 || status.equals("學生"))
   				price=(int)(price*0.8);

   				System.out.println(price);

    }
}



//


import java.util.*;

class Main {
    public static void main(String[] args) {

   		int price=200;
   		int age = 18;
   		String status="學生";
   			if(age<20)
   				price=(int)(price*0.8);
   			else if(status.equals("學生"))
   				price=(int)(price*0.8);

   			System.out.println(price);

    }
}
