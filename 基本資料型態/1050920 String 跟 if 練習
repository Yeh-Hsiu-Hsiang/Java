import java.lang.*;


class Main {
  	public static void main(String[] args) {
  		String s = "hello";
  		String ss = "How are you?";

  		System.out.println(s+" "+ss);


  }
}


//


import java.lang.*;


class Main {
  	public static void main(String[] args) {
  		String s = "hello";
  		String ss = "How are you?";
  		String a = s.concat(ss);

  			System.out.println(a);


  }
}

