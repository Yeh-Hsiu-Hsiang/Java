(3)

class Main {
  public static void main(String[] args) {

 	int i, j,a,aa;
 		i = 10;
 		j = 15;

		a = Math.max(i,j);
		aa = Math.min(i,j);

			System.out.println(a); //印出較大
			System.out.println(aa); //印出較小

  }
}

(2)-1


class Main {
  public static void main(String[] args) {

 	double i,j,k;
 		i = 2.5;
 		j = 7.3;
 		k = 1.6;
 			System.out.println((int)i*j+k);



  }
}


(2)-2


class Main {
  public static void main(String[] args) {

 	double i,j,k;
 		i = 2.5;
 		j = 7.3;
 		k = 1.6;
 			System.out.println(Math.ceil(i*j+k));



  }
}

(2)-3


class Main {
  public static void main(String[] args) {

 	double i,j,k;
 		i = 2.5;
 		j = 7.3;
 		k = 1.6;
 			System.out.println(Math.round(i*j+k));



  }
}

