class Main {
  public static void main(String[] args) {
	int r = 12;
	double rr =3.1416;
	double a = r * r * rr; //圓形面積

		System.out.println(a);

  }
}



//

class Main {
  public static void main(String[] args) {
	int r = 12;
	double rr =3.1416;
	double a = r * r * Math.PI; //圓形面積

		System.out.println(a);

  }
}



