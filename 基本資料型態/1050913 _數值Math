  public static void main(String[] args) {


 System.out.println(12*12*Math.PI);

  }
}



//

class Main {
  public static void main(String[] args) {

	int l = 15;
	int h = 9;
	int a = (l+h) * 2; //長方形周長

		System.out.println(a);

  }
}



//

class Main {
  public static void main(String[] args) {

	double l =8.5;
	double h = 8.2;
	double i = 7.4;

		double a = (l+h+i)*4; //正方形周長

			System.out.println(a);

  }
}



//

class Main {
  public static void main(String[] args) {

	int i = 15;
	double j = 7.2;

		double a = (i*i*Math.PI)-(j*j*Math.PI);  //大圓塗色面積(大圓-小圓)

			System.out.println(a);

  }
}



//


class Main {
  public static void main(String[] args) {

	double l =8.5;
	double h = 8.2;
	double i = 7.4;

		int a =(int)((l+h+i)*4); //小數轉換整數

			System.out.println(a);

  }
}



