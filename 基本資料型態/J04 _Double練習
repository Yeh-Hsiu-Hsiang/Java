class Main {
    public static void main(String[] args) {

     double total =12000;

     	if(total>10000){
     		total*=0.9;
     }

     	System.out.println(total);       //	System.out.println((int)total);

    }
}



//


import java.util.*;

class Main {
    public static void main(String[] args) {

     double total =18000;

     	if(total>10000){
     		total*=0.9;
     }else{
     		total*=0.95;
     }

     	System.out.println(total);  //System.out.println((int)total);

    }
}


